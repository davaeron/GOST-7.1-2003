    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp default",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "default"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp install",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "install"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}